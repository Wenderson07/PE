#include <stdio.h>
int main(){
	int fator1,fator2,resposta;
	resposta = 1;
		
	while(resposta ==1){
	printf("Digite um numero de 1 a 10: ");
	scanf("%d",&fator1);
	fator2=1;
	
	while(fator2 < 11){
	printf("%d X %d = %d\n",fator1,fator2, fator1  * fator2);
	fator2++;
	
	if (fator2 >=10.1){
		printf("Deseja imprimir outra tabuada <1>Sim <Outro valor>Nao: ");
		scanf("%d",&resposta);
		}
		}	
	}
	return 0;}
